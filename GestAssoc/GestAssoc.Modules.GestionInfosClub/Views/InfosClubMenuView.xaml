<RibbonTab x:Class="GestAssoc.Modules.GestionInfosClub.Views.InfosClubMenuView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:p="clr-namespace:GestAssoc.Modules.GestionInfosClub.Properties"
			 mc:Ignorable="d" 
		     Header="{x:Static p:Resources.MenuHeader}"
			 d:DesignHeight="120" d:DesignWidth="300">
	<RibbonGroup Header="{x:Static p:Resources.MenuHeader}">
		<RibbonButton Label="{x:Static p:Resources.MenuButton_Infos}"
						Command="{Binding Path=ShowDetailsInfosClub}"
						SmallImageSource="pack://application:,,,/GestAssoc.Common;component/Images/btn_list.png"/>
		<RibbonButton Label="{x:Static p:Resources.MenuButton_Edition}"
						Command="{Binding Path=ShowFormulaireInfosClub}"
						SmallImageSource="pack://application:,,,/GestAssoc.Common;component/Images/btn_listAdd.png"/>
	</RibbonGroup>
</RibbonTab>

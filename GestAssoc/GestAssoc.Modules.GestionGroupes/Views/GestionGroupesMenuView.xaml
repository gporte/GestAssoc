<RibbonGroup x:Class="GestAssoc.Modules.GestionGroupes.Views.GestionGroupesMenuView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:p="clr-namespace:GestAssoc.Modules.GestionGroupes.Properties"
			 mc:Ignorable="d" 
		     Header="{x:Static p:Resources.MenuHeader}"
			 d:DesignHeight="100" d:DesignWidth="150">
	<RibbonButton Label="{x:Static p:Resources.MenuButton_List}"
					SmallImageSource="pack://application:,,,/GestAssoc.Common;component/Images/btn_list.png"
					Command="{Binding Path=ShowConsultationGroupesCmd}" />
	<RibbonButton Label="{x:Static p:Resources.MenuButton_New}" 
					SmallImageSource="pack://application:,,,/GestAssoc.Common;component/Images/btn_listAdd.png"
					Command="{Binding Path=ShowFormulaireCreationGroupeCmd}" />
</RibbonGroup>
